      "signature": "checkUpkeep(bytes)",
      "name": "checkUpkeep",
      "description": "method that is simulated by the keepers to see if any work actually needs to be performed. This method does does not actually need to be executable, and since it is only ever simulated it can consume lots of gas. To ensure that it is never called, you may want to add the cannotExecute modifier from KeeperBase to your implementation of this method.",
      "parameterDecorators": [
        {
          "name": "checkData",
          "description": "specified in the upkeep registration so it is always the same for a registered upkeep. This can easily be broken down into specific arguments using `abi.decode`, so multiple upkeeps can be registered on the same contract and easily differentiated by the contract.",
          "recommendedTypes": []
        }
      ],
      "returnDecorators": [
        {
          "name": "upkeepNeeded",
          "description": "boolean to indicate whether the keeper should call performUpkeep or not.",
          "recommendedTypes": [],
          "solidityType": "bool"
        },
        {
          "name": "performData",
          "description": "bytes that the keeper should call performUpkeep with, if upkeep is needed. If you would like to encode data to decode later, try `abi.encode`.",
          "recommendedTypes": [],
          "solidityType": "bytes"
        }
      ],
