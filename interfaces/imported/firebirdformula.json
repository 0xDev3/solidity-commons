      "signature": "getAmountIn(uint256,uint256,uint256,uint32,uint32,uint32)",
      "name": "getAmountIn",
      "description": "given an output amount of an asset and pair reserves, returns a required input amount of the other asset Formula: return = reserveIn * ( (reserveOut / (reserveOut - amountOut)) ^ (tokenWeightOut / tokenWeightIn) - 1) * (10000/ (10000 - swapFee).",
      "parameterDecorators": [
        {
          "name": "amountOut",
          "description": "target reserve amount.",
          "recommendedTypes": []
        },
        {
          "name": "reserveIn",
          "description": "source reserve balance.",
          "recommendedTypes": []
        },
        {
          "name": "reserveOut",
          "description": "target reserve balance.",
          "recommendedTypes": []
        },
        {
          "name": "tokenWeightIn",
          "description": "source reserve weight, represented in ppm (2-98).",
          "recommendedTypes": []
        },
        {
          "name": "tokenWeightOut",
          "description": "target reserve weight, represented in ppm (2-98).",
          "recommendedTypes": []
        },
        {
          "name": "swapFee",
          "description": "swap fee of the conversion.",
          "recommendedTypes": []
        }
      ],
      "returnDecorators": [
        {
          "name": "amountIn",
          "description": "amountIn.",
          "recommendedTypes": [],
          "solidityType": "uint256"
        }
      ],
      "emittableEvents": [],
      "readOnly": true
    },
